@page "/listAllDogs"

@inject HttpClient Http

<h1>ListAllDogs</h1>
@if (data == null)
{
    <p><em>Loading...</em></p>
}
else
{
    <table class="table">
        <thead>
            <tr>
                <th>Date</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var forecast in data)
            {
                <tr>
                    <td>@data.Length </td>
                   
                </tr>
            }
        </tbody>
    </table>
}


